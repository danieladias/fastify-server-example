{
  "name": "fastify-server-example",
  "version": "0.0.1",
  "description": "Fastify API Example",
  "main": "src/server.ts",
  "author": "Farfetch",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=18.12.1"
  },
  "scripts": {
    "check-types": "tsc --noEmit",
